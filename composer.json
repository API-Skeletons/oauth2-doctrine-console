{
    "name": "api-skeletons/oauth2-doctrine-console",
    "description": "Console management of an OAuth2 for Doctrine Server for Laminas",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "console",
        "oauth2",
        "laminas"
    ],
    "require": {
        "php": "^7.3",
        "api-skeletons/oauth2-doctrine": "^5.0",
        "laminas/laminas-mvc-console": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeletons\\OAuth2\\Doctrine\\Console\\": "src/",
            "ApiSkeletonsTest\\OAuth2\\Doctrine\\Console\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "extra": {
        "laminas": {
            "component": "ApiSkeletons\\OAuth2\\Doctrine\\Console",
            "config-provider": "ApiSkeletons\\OAuth2\\Doctrine\\Console\\ConfigProvider"
        }
    }
}
